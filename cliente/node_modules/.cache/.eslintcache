   <Col span={12}>\r\n          <Form.Item>\r\n            <Select\r\n              placeholder=\"Seleccione un rol\"\r\n              value={userData.role}\r\n              onChange={(e) => setUserData({ ...userData, role: e })}\r\n            >\r\n              <Option value=\"admin\">Administrador</Option>\r\n              <Option value=\"editor\">Editor</Option>\r\n              <Option value=\"reviwer\">Revisor</Option>\r\n              \r\n            </Select>\r\n          </Form.Item>\r\n        </Col>\r\n      </Row>\r\n      <Row gutter={24}>\r\n        <Col span={12}>\r\n          <Form.Item>\r\n            <Input\r\n              prefix={<UserOutlined />}\r\n              type=\"password\"\r\n              placeholder=\"contraseña\"\r\n              value={userData.password}\r\n              onChange={(e) => setUserData({ ...userData, password: e.target.value })}              \r\n            />\r\n          </Form.Item>\r\n        </Col>\r\n        <Col span={12}>\r\n          <Form.Item>\r\n            <Input\r\n              prefix={<UserOutlined />}\r\n              type=\"password\"\r\n              placeholder=\"Repetir contraseña\"\r\n              value={userData.repetPassword}\r\n              onChange={(e) => setUserData({ ...userData, repetPassword: e.target.value })}              \r\n            />\r\n          </Form.Item>\r\n        </Col>\r\n      </Row>\r\n      <Form.Item>\r\n        <Button type=\"primary\" className=\"btn-submit\" onClick={createUser}>\r\n          Crear Usuario\r\n        </Button>\r\n      </Form.Item>\r\n    </Form>\r\n  );\r\n}\r\n","C:\\CURSOSUDEMY\\PRACTICOS\\CURSO MERN\\ProyectoWebPersonal\\web-personal-cliente\\src\\pages\\Admin\\MenuWeb\\index.js",[],"C:\\CURSOSUDEMY\\PRACTICOS\\CURSO MERN\\ProyectoWebPersonal\\web-personal-cliente\\src\\pages\\Admin\\MenuWeb\\MenuWeb.js",["208"],"C:\\CURSOSUDEMY\\PRACTICOS\\CURSO MERN\\ProyectoWebPersonal\\web-personal-cliente\\src\\api\\menu.js",[],"C:\\CURSOSUDEMY\\PRACTICOS